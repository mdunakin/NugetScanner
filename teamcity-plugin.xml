<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>NugetScanner</name>
    <display-name>NuGet Vulnerability Scanner</display-name>
    <version>1.0.0</version>
    <description>Agent-side scanner that reports vulnerable or deprecated NuGet packages as TeamCity build problems.</description>
  </info>
  <requirements>
    <min-build>2022.04</min-build>
  </requirements>
  <deployment use-separate-classloader="false" />
  <agents>
    <agent />
  </agents>
</teamcity-plugin>
